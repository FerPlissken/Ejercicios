package SmartDevices;

public class SmartDevice {
    String marca;
    int year;
    String modelo;

    public SmartDevice() {
    }

    public SmartDevice(String marca, int year, String modelo) {
        this.marca = marca;
        this.year = year;
        this.modelo = modelo;
    }

    public String getMarca() {
        return marca;
    }

    public int getYear() {
        return year;
    }

    public String getModelo() {
        return modelo;
    }

    public void setMarca(String marca) {
        this.marca = marca;
    }

    public void setYear(int year) {
        this.year = year;
    }

    public void setModelo(String modelo) {
        this.modelo = modelo;
    }
}
