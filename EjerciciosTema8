public class Persona {

        private int edad;
        private String nombre;
        private int telf;

    public void setEdad (int edad){
        this.edad = edad;    }
    public int getEdad(){
        return this.edad;
    }

    public void setNombre (String nombre){
        this.nombre = nombre;
    }
    public String getNombre(){
        return this.nombre;
    }
    public void setTelf (int telf){
        this.telf = telf;
    }
    public int getTelf(){
        return this.telf;
        }

    public static void main(String[] args) {

        Persona edad = new Persona();
        edad.setEdad (23);

        Persona nombre = new Persona();
        nombre.setNombre ("Anastasio");

        Persona telf = new Persona();
        telf.setTelf(902202122);

        System.out.println("Edad: " + edad.getEdad());
        System.out.println("Nombre: " + nombre.getNombre());
        System.out.println("Tel√©fono: " + telf.getTelf());

    }}
